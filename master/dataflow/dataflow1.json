{
	"name": "dataflow1",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Filmfreeway_Submissions_Film",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"linkedService": {
						"referenceName": "AzureSqlDatabaseRAW_ATLFS_DATA",
						"type": "LinkedServiceReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          {Project Website} as string,",
				"          Twitter as string,",
				"          Facebook as string,",
				"          {Country of Origin} as string,",
				"          Language as string,",
				"          {Trailer URL} as string,",
				"          {Country of Filming} as string,",
				"          {Project Type} as string,",
				"          Genres as string,",
				"          {Student Project} as boolean,",
				"          {Completion Date} as date,",
				"          {Production Budget} as string,",
				"          {Shooting Format} as string,",
				"          {Aspect Ratio} as string,",
				"          {Film Color} as string,",
				"          Camera as string,",
				"          Lens as string,",
				"          {Focal Length} as string,",
				"          {Shutter Speed} as string,",
				"          Aperture as string,",
				"          {ISO / Film} as string,",
				"          {First-time Filmmaker} as boolean,",
				"          Directors as string,",
				"          Writers as string,",
				"          Producers as string,",
				"          {Key Cast} as string,",
				"          {Other Credits} as string,",
				"          {Screenings / Awards} as string,",
				"          {Distributor Information} as string,",
				"          {First Name} as string,",
				"          {Last Name} as string,",
				"          Birthdate as date,",
				"          Gender as string,",
				"          Email as string,",
				"          Phone as string,",
				"          Address as string,",
				"          {Address 2} as string,",
				"          City as string,",
				"          State as string,",
				"          {Postal Code} as string,",
				"          Country as string,",
				"          {Tracking Number} as string,",
				"          {Project Title} as string,",
				"          {Project Title (Original Language)} as string,",
				"          Synopsis as string,",
				"          {Synopsis (Original Language)} as string,",
				"          Lyrics as string,",
				"          Duration as string,",
				"          {Submission Date} as date,",
				"          {Submission Status} as string,",
				"          {Judging Status} as string,",
				"          {Submission Deadlines} as string,",
				"          {Submission Categories} as string,",
				"          {Submission Fee} as double,",
				"          {Discount Code} as string,",
				"          {Submission Link} as string,",
				"          {Submission Password} as string,",
				"          {Assigned Judges} as string,",
				"          {Submission ID} as integer,",
				"          {Submission Notes} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table',",
				"     store: 'sqlserver',",
				"     schemaName: 'Filmfreeway_FILMS',",
				"     tableName: 'Test',",
				"     insertable: true,",
				"     updateable: false,",
				"     deletable: false,",
				"     upsertable: false,",
				"     recreate: true,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     saveOrder: 1,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}