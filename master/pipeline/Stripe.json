							{
								"source": {
									"path": "$['url']"
								}
							}
						],
						"collectionReference": "$['data']",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "StripeCharges",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SQLStripeCharges",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}