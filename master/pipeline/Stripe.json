							{
								"source": {
									"path": "['card']"
								}
							},
							{
								"source": {
									"path": "['created']"
								},
								"sink": {
									"name": "Created (UTC)",
									"type": "DateTime"
								}
							},
							{
								"source": {
									"path": "['currency']"
								},
								"sink": {
									"name": "Currency",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['customer']"
								},
								"sink": {
									"name": "Customer ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['description']"
								},
								"sink": {
									"name": "Description",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['destination']"
								},
								"sink": {
									"name": "Destination",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['dispute']"
								},
								"sink": {
									"name": "Dispute Status",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['fee']"
								},
								"sink": {
									"name": "Fee",
									"type": "Double"
								}
							},
							{
								"source": {
									"path": "['invoice']"
								},
								"sink": {
									"name": "Invoice ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['livemode']"
								},
								"sink": {
									"name": "Mode",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['id']"
								},
								"sink": {
									"name": "id (metadata)",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['metadata']['websiteId']"
								},
								"sink": {
									"name": "websiteId (metadata)",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['order']"
								},
								"sink": {
									"name": "orderId (metadata)",
									"type": "String"
								}
