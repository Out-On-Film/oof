					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['id']"
								},
								"sink": {
									"name": "id",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['amount']"
								},
								"sink": {
									"name": "Amount",
									"type": "Double"
								}
							},
							{
								"source": {
									"path": "['amount_refunded']"
								},
								"sink": {
									"name": "Amount Refunded",
									"type": "Double"
								}
							},
							{
								"source": {
									"path": "['billing_details']['address']['city']"
								},
								"sink": {
									"name": "Card Address City",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['billing_details']['address']['country']"
								},
								"sink": {
									"name": "Card Address Country",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['billing_details']['address']['line1']"
								},
								"sink": {
									"name": "Card Address Line1",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['billing_details']['address']['line2']"
								},
								"sink": {
									"name": "Card Address Line2",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['billing_details']['address']['postal_code']"
								},
								"sink": {
									"name": "Card Address Zip",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['billing_details']['address']['state']"
								},
								"sink": {
									"name": "Card Address State",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['calculated_statement_descriptor']"
								},
								"sink": {
									"name": "Statement Descriptor",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['captured']"
								},
								"sink": {
									"name": "Captured",
