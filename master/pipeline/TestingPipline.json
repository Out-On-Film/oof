{
	"name": "TestingPipline",
	"properties": {
		"activities": [
			{
				"name": "Venues",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET"
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"Venue ID"
							]
						},
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "['id']"
								},
								"sink": {
									"name": "Venue ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['event_bucket']"
								},
								"sink": {
									"name": "Event Bucket",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['name']"
								},
								"sink": {
									"name": "Venue Name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['short_name']"
								},
								"sink": {
									"name": "Short Name",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['description']"
								},
								"sink": {
									"name": "Short Description",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['address']"
								},
								"sink": {
									"name": "Address",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['default_capacity']"
								},
								"sink": {
									"name": "Capacity",
									"type": "Int32"
								}
							},
							{
								"source": {
									"path": "['color']"
								},
								"sink": {
									"name": "Color",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "['use_reserved_seating']"
								},
								"sink": {
									"name": "Use Reserved Seating",
									"type": "Boolean"
								}
							}
						],
						"collectionReference": "$['venues']"
					}
				},
				"inputs": [
					{
						"referenceName": "EventiveVenues",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlEventiveVenues",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}