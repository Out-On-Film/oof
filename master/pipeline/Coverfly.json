{
	"name": "Coverfly",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"additionalHeaders": {
							"X-Authorization": "aFE2yXRTRvFKxRoZjelr8ueCK4s259oe"
						},
						"paginationRules": {
							"QueryParameters.{offset}": "RANGE:0:2000:50"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"Coverfly ID"
							]
						},
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['id']"
								},
								"sink": {
									"name": "Coverfly ID",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['title']"
								},
								"sink": {
									"name": "Title",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['email']"
								},
								"sink": {
									"name": "Submitter Email",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['first_name']"
								}
							},
							{
								"source": {
									"path": "$['last_name']"
								}
							},
							{
								"source": {
									"path": "$['writers'][0]['first_name']"
								}
							},
							{
								"source": {
									"path": "$['writers'][0]['last_name']"
								}
							},
							{
								"source": {
									"path": "$['package']"
								}
							},
							{
								"source": {
									"path": "$['eliminated']"
								}
							},
							{
								"source": {
									"path": "$['has_file']"
								}
							},
							{
								"source": {
									"path": "$['placement']"
								}
							},
							{
								"source": {
									"path": "$['network']"
								}
							},
							{
								"source": {
									"path": "$['paid']"
								}
							},
							{
								"source": {
									"path": "$['score']"
								}
							}
						]
					}
				},
				"inputs": [
					{
						"referenceName": "CoverflyScripts",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlSUBMISSIONS_COVERFLY_Screenplays",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}